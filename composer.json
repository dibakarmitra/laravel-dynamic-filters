{
    "name": "dibakar/laravel-dynamic-filters",
    "description": "A flexible filtering system for Laravel Eloquent models",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dibakar",
            "email": "dibakar@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Dibakar\\LaravelDynamicFilters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dibakar\\LaravelDynamicFilters\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dibakar\\LaravelDynamicFilters\\DynamicFiltersServiceProvider"
            ],
            "aliases": {
                "DynamicFilter": "Dibakar\\LaravelDynamicFilters\\Facades\\DynamicFilter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
