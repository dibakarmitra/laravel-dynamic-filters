{
    "name": "dibakar/laravel-dynamic-filters",
    "description": "A flexible and dynamic filtering system for Laravel Eloquent models that allows for easy implementation of complex filtering and searching capabilities in your applications.",
    "keywords": ["laravel", "filtering", "searching", "eloquent", "dynamic filters", "filters"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dibakar Mitra",
            "email": "mitradibakar@outlook.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dibakar\\LaravelDynamicFilters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dibakar\\LaravelDynamicFilters\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dibakar\\LaravelDynamicFilters\\DynamicFiltersServiceProvider"
            ],
            "aliases": {
                "DynamicFilter": "Dibakar\\LaravelDynamicFilters\\Facades\\DynamicFilter"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "version": "1.0.0",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dibakarmitra/laravel-dynamic-filters"
        }
    ]
}
